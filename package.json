{
  "name": "create-babylon-app",
  "version": "0.2.0",
  "description": "A CLI for scaffolding Babylon.js Web App.",
  "type": "commonjs",
  "main": "index.cjs",
  "bin": {
    "create-babylon-app": "index.cjs"
  },
  "homepage": "https://github.com/drumath2237/create-babylon-app",
  "repository": {
    "url": "git+https://github.com/drumath2237/create-babylon-app.git",
    "type": "git"
  },
  "scripts": {
    "dev": "vite-node src/index.ts",
    "build": "tsc -b",
    "start": "node ./index.cjs",
    "version:set": "pnpm exec lerna version",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "check": "biome check ./src",
    "check:write": "biome check --write ./src",
    "check:ci": "biome ci ./src"
  },
  "author": "@drumath2237",
  "license": "Apache-2.0",
  "dependencies": {
    "@inquirer/prompts": "^5.0.5",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@lerna-lite/cli": "^3.5.1",
    "@lerna-lite/version": "^3.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.2",
    "typescript": "^5.4.5",
    "vite-node": "^1.6.0"
  },
  "keywords": [
    "babylonjs",
    "babylonjs",
    "CLI",
    "vite",
    "nodejs",
    "typescript"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/",
    "templates/",
    "index.cjs"
  ]
}
